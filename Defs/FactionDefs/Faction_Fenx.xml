<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <FactionDef>
		<defName>FennexFaction</defName>
		<label>Fennex Cooperative</label>
    <description>A colony of the mischevious Fennex race.</description>
    <pawnsPlural>Fennex</pawnsPlural>
    <requiredCountAtGameStart>1</requiredCountAtGameStart>
    <settlementGenerationWeight>1</settlementGenerationWeight>
    <canMakeRandomly>true</canMakeRandomly>
    <canSiege>true</canSiege>
    <canStageAttacks>true</canStageAttacks>
    <leaderTitle>Archivist</leaderTitle>
    <startingGoodwill>
      <min>10</min>
      <max>40</max>
    </startingGoodwill>
	<factionNameMaker>NamerFactionOutlander</factionNameMaker>
	<settlementNameMaker>NamerSettlementOutlander</settlementNameMaker>
	<techLevel>Ultra</techLevel>
  <backstoryCategories>
			<li>FennexBackstory</li>
  	</backstoryCategories>
			<hairTags>
			<li>Urban</li>
      <li>Rural</li>
      <li>Tribal</li>
      <li>Punk</li>
			</hairTags>
    <naturalColonyGoodwill>
      <min>-60</min>
      <max>0</max>
    </naturalColonyGoodwill>
    <visitorTraderKinds>
      <li>Visitor_Outlander_Standard</li>
    </visitorTraderKinds>
   <caravanTraderKinds>
      <li>Caravan_Outlander_BulkGoods</li>
      <li>Caravan_Outlander_CombatSupplier</li>
      <li>Caravan_Outlander_Exotic</li>
    </caravanTraderKinds>
    <baseTraderKinds>
      <li>Base_Outlander_Standard</li>
    </baseTraderKinds>
    <allowedArrivalTemperatureRange>
      <min>-50</min>
      <max>45</max>
    </allowedArrivalTemperatureRange>
    <maxPawnCostPerTotalPointsCurve>
      <points>
        <li>(0,35)</li>
        <li>(70, 50)</li>
        <li>(700, 100)</li>
        <li>(1300, 150)</li>
        <li>(100000, 10000)</li>
      </points>
    </maxPawnCostPerTotalPointsCurve>
    <pawnGroupMakers>
      <li>
        <kindDef>Combat</kindDef>
        <options>
          <FennexColonist>50</FennexColonist>
          <FennexSecurityGuard>90</FennexSecurityGuard>
          <FennexKeeper>250</FennexKeeper>
          <FennexSoldier>100</FennexSoldier>
          <FennexStalker>130</FennexStalker>
          <FennexAgent>130</FennexAgent>
          <FennexAssassin>170</FennexAssassin>
        </options>
      </li>
       <li>
        <kindDef>Peaceful</kindDef>
        <options>
          <FennexColonist>50</FennexColonist>
          <FennexSecurityGuard>90</FennexSecurityGuard>
          <FennexKeeper>250</FennexKeeper>
          <FennexSoldier>100</FennexSoldier>
          <FennexStalker>130</FennexStalker>
        </options>
      </li>
	  <li>
      <kindDef>Trader</kindDef>
        <traders>
          <FennexTrader>1</FennexTrader>
        </traders>
        <carriers>
          <Muffalo>2</Muffalo>
          <Dromedary>2</Dromedary>
        </carriers>
        <guards>
          <FennexSecurityGuard>90</FennexSecurityGuard>
          <FennexSoldier>100</FennexSoldier>
          <FennexStalker>130</FennexStalker>
          <FennexAgent>130</FennexAgent>
        </guards>
      </li>
	  <li>
        <kindDef>Settlement</kindDef>
        <options>
          <FennexColonist>50</FennexColonist>
          <FennexSecurityGuard>90</FennexSecurityGuard>
          <FennexKeeper>250</FennexKeeper>
          <FennexSoldier>100</FennexSoldier>
          <FennexStalker>130</FennexStalker>
          <FennexAgent>130</FennexAgent>
          <FennexAssassin>170</FennexAssassin>
        </options>
      </li>
    </pawnGroupMakers>
    <homeIconPath>World/FennexBaseIcon</homeIconPath>
	<expandingIconTexture>World/FennexBaseIcon</expandingIconTexture>
  </FactionDef>

</Defs>